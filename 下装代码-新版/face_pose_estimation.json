{
    "name": "face_pose_estimation",  
    "flow": ["face_detection", "face_alignment"],
    "model_ctx": {  
        "face_detection": {
            "net_type": "mb_tiny_RFB_fd",
            "input_size": 480,
            "threshold": 0.7,
            "candidate_size": 1500,
            "device": "cpu"
        },
        "face_alignment": {
            "lite_version": 1,
            "model_path": "models/hopenet_lite_6MB.pkl",
            "batch_size": 1,
            "device": "cpu"
        }
    },
    "input_ctx": {  
        "face_detection": ["image"],
        "face_alignment": ["image", "bbox", "prob"],
        "R": ["image", "bbox", "head_pose"]
    },
    "output_ctx": {  
        "face_detection": ["image", "bbox", "prob"],
        "face_alignment": ["image", "bbox", "head_pose"],
        "R": []
    },
    "task_list": ["face_detection", "face_alignment"],
    "task_dag": { 
        "face_detection" : [],
        "face_alignment" : ["face_detection"]
    },
    "task_code_path": { 
        "face_detection" : "/app/face_detection",
        "face_alignment" : "/app/face_alignment"
    },
    "Accuracy": 0.6,  
    "Latency": 0.5,  
    "priority": 0  
}